-
-        if (!indicator || !strengthText) return;
-
-        let strength = 0;
-        let text = '';
-        let className = '';
-
-        if (password.length === 0) {
-            indicator.style.width = '0%';
-            strengthText.textContent = '';
-            indicator.className = 'progress-bar';
-            return;
-        }
-
-        // Calculate strength
-        if (password.length >= 8) strength++;
-        if (password.length >= 12) strength++;
