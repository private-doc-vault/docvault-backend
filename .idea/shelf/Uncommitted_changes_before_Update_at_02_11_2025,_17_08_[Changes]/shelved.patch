-        $shares = $shareRepository->findBy([
-            'sharedWith' => $user,
-            'isActive' => true
-        ]);
-
-        // Filter out expired shares and extract documents
-        $sharedDocuments = [];
-        foreach ($shares as $share) {
-            if (!$share->isExpired()) {
-                $sharedDocuments[] = $share->getDocument();
-            }
-        }
