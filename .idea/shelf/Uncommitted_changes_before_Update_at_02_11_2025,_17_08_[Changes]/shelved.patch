-                if (unlink($filePath)) {
-                    $deletedCount++;
-                    $this->logger->info('Deleted file for old document', [
-                        'document_id' => $document->getId(),
-                        'file_path' => $filePath,
-                        'updated_at' => $document->getUpdatedAt()?->format('Y-m-d H:i:s')
-                    ]);
-                } else {
