-            return;
-        }
-
-        // Handle generic exceptions (only in dev/test environment for security)
-        if ($_ENV['APP_ENV'] !== 'prod') {
-            $response = new JsonResponse([
-                'error' => 'Internal server error',
-                'message' => $exception->getMessage(),
