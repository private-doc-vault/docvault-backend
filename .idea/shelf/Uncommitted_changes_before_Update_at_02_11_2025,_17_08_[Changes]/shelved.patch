-                $connection->executeStatement('SET session_replication_role = replica;');
-            } else {
-                $connection->executeStatement('SET FOREIGN_KEY_CHECKS = 0;');
-            }
-
-            // Clear tables in order to avoid foreign key constraint violations
-            $tables = [
