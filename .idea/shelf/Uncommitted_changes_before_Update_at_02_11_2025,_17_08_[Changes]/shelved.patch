-                ], $doc->getTags()->toArray()),
-            ];
-        }, $documents);
-
-        return new JsonResponse([
-            'documents' => $documentData,
-            'total' => $totalCount,
-            'limit' => $limit,
-            'offset' => $offset,
-        ]);
-    }
-
-    /**
