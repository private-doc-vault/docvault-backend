-        return [
-            'canRead' => $this->canRead($user, $document),
-            'canWrite' => $this->canWrite($user, $document),
-            'canDelete' => $this->canDelete($user, $document),
-            'canShare' => $this->canShare($user, $document),
-            'isOwner' => $isOwner,
